{
  "compilerOptions": {
    "target": "esnext",
    "module": "ESNext",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "strict": true,
    "strictNullChecks": true,
    "lib": [
      "ES2022",
      "webworker"
    ],
    "declaration": true,
    "declarationMap": true
  }
}
